{
  "name": "@vitebook/client",
  "version": "0.0.0",
  "keywords": [
    "client",
    "esm",
    "plugin",
    "storybook",
    "vitebook"
  ],
  "bugs": {
    "url": "https://github.com/vitebook/vitebook/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vitebook/vitebook.git",
    "directory": "packages/client"
  },
  "license": "MIT",
  "contributors": [
    "Rahim Alwer <rahim.alwer@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./src/client/index.ts",
    "./*": "./src/client/*",
    "./node": "./src/node/index.ts",
    "./node/*": "./src/node/*",
    "./shim": "./src/shim.d.ts",
    "./shim.d.ts": "./src/shim.d.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "node": [
        "src/node/index.ts"
      ],
      "node/*": [
        "src/node/*"
      ],
      "shim": [
        "src/shim.d.ts"
      ],
      "*": [
        "src/client/*"
      ]
    }
  },
  "files": [
    "src/"
  ],
  "dependencies": {
    "@rollup/pluginutils": "^4.1.1",
    "@vitejs/plugin-vue": "^1.8.0",
    "quicklink": "^2.2.0",
    "vue": "^3.2.6",
    "vue-router": "^4.0.11",
    "vue-shadow-dom": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
