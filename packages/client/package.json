{
  "name": "@vitebook/client",
  "version": "0.1.1",
  "keywords": [
    "client",
    "esm",
    "plugin",
    "storybook",
    "svelte",
    "vitebook"
  ],
  "bugs": {
    "url": "https://github.com/vitebook/vitebook/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vitebook/vitebook.git",
    "directory": "packages/client"
  },
  "license": "MIT",
  "contributors": [
    "Rahim Alwer <rahim.alwer@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/client/index.js",
    "./*": "./dist/client/*",
    "./node": "./dist/node/index.js",
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "node": [
        "dist/types/node/index.d.ts"
      ],
      "*": [
        "dist/types/client/*"
      ]
    }
  },
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "build": "run-s clean build:client build:node build:types",
    "build:client": "node ../../.scripts/build-client.js --entry src/client/index.ts,src/client/entry-client.ts,src/client/entry-server.ts",
    "build:node": "node ../../.scripts/build-node.js --external @sveltejs/vite-plugin-svelte,magic-string,svelte-preprocess-esbuild",
    "build:types": "tsc -p tsconfig.json --outDir dist/types --emitDeclarationOnly",
    "clean": "rimraf dist",
    "dev": "pnpm clean && run-p watch:client watch:node watch:types",
    "watch:client": "pnpm build:client -- --watch",
    "watch:node": "pnpm build:node -- --watch",
    "watch:types": "pnpm build:types -- --watch"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.1.1",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.26",
    "@vitebook/core": "0.1.1",
    "esbuild": "^0.12.24",
    "kleur": "^4.1.4",
    "magic-string": "^0.25.7",
    "svelte": "^3.43.2",
    "svelte-preprocess-esbuild": "^2.0.0"
  },
  "devDependencies": {
    "vite": "2.7.0-beta.0"
  },
  "peerDependencies": {
    "vite": "^2.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
