{
  "name": "@vitebook/client",
  "version": "0.0.3",
  "keywords": [
    "client",
    "esm",
    "plugin",
    "storybook",
    "svelte",
    "vitebook"
  ],
  "bugs": {
    "url": "https://github.com/vitebook/vitebook/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vitebook/vitebook.git",
    "directory": "packages/client"
  },
  "license": "MIT",
  "contributors": [
    "Rahim Alwer <rahim.alwer@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./src/client/index.ts",
    "./*": "./src/client/*",
    "./node": "./dist/index.js",
    "./shim": "./src/shim.d.ts",
    "./shim.d.ts": "./src/shim.d.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "node": [
        "src/node/index.ts"
      ],
      "shared": [
        "src/shared/index.ts"
      ],
      "shim": [
        "src/shim.d.ts"
      ],
      "*": [
        "src/client/*"
      ]
    }
  },
  "files": [
    "src/",
    "dist/"
  ],
  "scripts": {
    "build": "npm run clean && node .scripts/build.mjs",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.1.1",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.26",
    "@vitebook/core": "0.0.3",
    "esbuild": "^0.12.24",
    "kleur": "^4.1.4",
    "magic-string": "^0.25.7",
    "svelte": "^3.43.2",
    "svelte-preprocess-esbuild": "^2.0.0"
  },
  "devDependencies": {
    "vite": "^2.5.3"
  },
  "peerDependencies": {
    "vite": "^2.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
