{
  "name": "@vitebook/markdown",
  "version": "0.0.3",
  "keywords": [
    "esm",
    "markdown",
    "plugin",
    "vitebook"
  ],
  "bugs": {
    "url": "https://github.com/vitebook/vitebook/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vitebook/vitebook.git",
    "directory": "packages/markdown"
  },
  "license": "MIT",
  "contributors": [
    "Rahim Alwer <rahim.alwer@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./node": "./dist/index.js",
    "./shared": "./src/shared/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "node": [
        "./src/node/index.ts"
      ],
      "shared": [
        "./src/shared/index.ts"
      ],
      "*": [
        "src/node/index.ts"
      ]
    }
  },
  "files": [
    "src/",
    "dist/"
  ],
  "scripts": {
    "build": "npm run clean && node .scripts/build.mjs",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.1.1",
    "@types/lru-cache": "^5.1.1",
    "@types/markdown-it": "^12.2.1",
    "gray-matter": "^4.0.3",
    "lru-cache": "^6.0.0",
    "markdown-it": "^12.2.0",
    "markdown-it-anchor": "^8.3.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-emoji": "^2.0.0",
    "node-html-parser": "^5.0.0",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@vitebook/client": "workspace:*",
    "@vitebook/core": "workspace:*"
  },
  "peerDependencies": {
    "@vitebook/client": "0.0.3",
    "@vitebook/core": "0.0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
