{
  "name": "@vitebook/vue",
  "version": "0.0.0",
  "keywords": [
    "client",
    "esm",
    "plugin",
    "storybook",
    "vitebook",
    "vue",
    "vuebook"
  ],
  "bugs": {
    "url": "https://github.com/vitebook/vitebook/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vitebook/vitebook.git",
    "directory": "packages/vue"
  },
  "license": "MIT",
  "contributors": [
    "Rahim Alwer <rahim.alwer@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*",
    "./client": "./src/client/index.ts",
    "./client/*": "./src/client/*",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "client": [
        "src/client/index.ts"
      ],
      "client/*": [
        "src/client/*"
      ],
      "*": [
        "src/*"
      ]
    }
  },
  "files": [
    "src/"
  ],
  "dependencies": {
    "@rollup/pluginutils": "^4.1.1",
    "@vitejs/plugin-vue": "^1.6.1",
    "quicklink": "^2.2.0",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "vue": "^3.2.6"
  },
  "peerDependencies": {
    "vue": "^3.2.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
