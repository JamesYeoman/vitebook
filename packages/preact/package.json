{
  "name": "@vitebook/preact",
  "version": "0.0.0",
  "keywords": [
    "esm",
    "plugin",
    "preact",
    "react",
    "storybook",
    "vitebook"
  ],
  "bugs": {
    "url": "https://github.com/vitebook/vitebook/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vitebook/vitebook.git",
    "directory": "packages/preact"
  },
  "license": "MIT",
  "contributors": [
    "Rahim Alwer <rahim.alwer@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./src/client/index.js",
    "./*": "./src/client/*",
    "./node": "./dist/index.js",
    "./shared": "./src/shared/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "client": [
        "src/client/index.ts"
      ],
      "client/*": [
        "src/client/*"
      ],
      "node": [
        "src/node/index.ts"
      ],
      "shared": [
        "src/shared/index.ts"
      ]
    }
  },
  "files": [
    "src/",
    "dist/",
    ".scripts/patch-prefresh.mjs"
  ],
  "scripts": {
    "build": "npm run clean && node .scripts/build.mjs",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf dist",
    "postinstall": "node .scripts/patch-prefresh.mjs"
  },
  "dependencies": {
    "@babel/core": "^7.15.8",
    "@prefresh/core": "^1.3.1",
    "@prefresh/vite": "^2.2.3",
    "@types/babel__core": "^7.1.16",
    "babel-plugin-transform-hook-names": "^1.0.2",
    "kleur": "^4.1.4",
    "preact": "^10.5.14"
  },
  "devDependencies": {
    "@rollup/pluginutils": "^4.1.1",
    "@vitebook/client": "workspace:*",
    "@vitebook/core": "workspace:*",
    "svelte": "^3.44.0",
    "vite": "^2.5.3"
  },
  "peerDependencies": {
    "@vitebook/client": "workspace:*",
    "@vitebook/core": "workspace:*",
    "svelte": "^3.44.0",
    "vite": "^2.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
