{
  "name": "@vitebook/core",
  "version": "0.0.0",
  "keywords": [
    "alternative",
    "core",
    "esm",
    "fast",
    "lightweight",
    "modern",
    "storybook",
    "vite",
    "vitebook"
  ],
  "bugs": {
    "url": "https://github.com/vitebook/vitebook/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vitebook/vitebook.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "contributors": [
    "Rahim Alwer <rahim.alwer@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/node/index.js",
    "./node": "./dist/node/index.js",
    "./node/*": "./dist/node/*",
    "./node/utils": "./dist/node/utils/index.js",
    "./shared": "./dist/shared/index.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "node": [
        "dist/node/index.d.ts"
      ],
      "node/*": [
        "dist/node/*"
      ],
      "shared": [
        "dist/shared/index.d.ts"
      ]
    }
  },
  "bin": {
    "vitebook": "bin/vitebook.js"
  },
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "build:watch": "npm run build -- -w",
    "clean": "rimraf *.tsbuildinfo dist"
  },
  "dependencies": {
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.12",
    "@types/lru-cache": "^5.1.1",
    "cac": "^6.7.3",
    "chokidar": "^3.5.2",
    "debug": "^4.3.2",
    "esbuild": "^0.12.24",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0",
    "just-omit": "^1.2.0",
    "kleur": "^4.1.4",
    "lru-cache": "^6.0.0",
    "ora": "^6.0.0",
    "upath": "^2.0.1",
    "vite": "^2.5.3"
  },
  "engines": {
    "node": ">=14.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
