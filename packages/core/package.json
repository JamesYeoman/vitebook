    "kleur": "^4.1.4",
    "lru-cache": "^6.0.0",
    "ora": "^6.0.0",
    "pkg-up": "^2.0.0",
    "upath": "^2.0.1",
    "vite": "^2.5.3"
  },
  "devDependencies": {
    "rollup": "^2.58.0"
  },
  "peerDependencies": {
    "rollup": "^2.58.0"
