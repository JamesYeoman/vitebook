{
  "name": "@vitebook/core",
  "version": "0.0.2",
  "keywords": [
    "alternative",
    "core",
    "esm",
    "fast",
    "lightweight",
    "modern",
    "storybook",
    "vite",
    "vitebook"
  ],
  "bugs": {
    "url": "https://github.com/vitebook/vitebook/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vitebook/vitebook.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "contributors": [
    "Rahim Alwer <rahim.alwer@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./node": "./dist/index.js",
    "./node/utils": "./dist/utils/index.js",
    "./shared": "./src/shared/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "node": [
        "src/node/index.ts"
      ],
      "node/utils": [
        "src/node/utils/index.ts"
      ],
      "shared": [
        "src/shared/index.ts"
      ]
    }
  },
  "bin": {
