{
  "name": "@vitebook/theme-default",
  "version": "0.0.3",
  "keywords": [
    "esm",
    "plugin",
    "storybook",
    "theme-default",
    "vitebook"
  ],
  "bugs": {
    "url": "https://github.com/vitebook/vitebook/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vitebook/vitebook.git",
    "directory": "packages/theme-default"
  },
  "license": "MIT",
  "contributors": [
    "Rahim Alwer <rahim.alwer@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./src/client/index.ts",
    "./*": "./src/client/*",
    "./node": "./dist/index.js",
    "./node/*": "./src/node/*",
    "./shim": "./src/shim.d.ts",
    "./shim.d.ts": "./src/shim.d.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "node": [
        "src/node/index.ts"
      ],
      "shim": [
        "src/shim.d.ts"
      ],
      "*": [
        "src/client/*"
      ]
    }
  },
  "files": [
    "src/",
    "dist/"
  ],
  "scripts": {
    "build": "npm run clean && node .scripts/build.mjs && npm run copy:icons",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf dist",
    "copy:icons": "node .scripts/copy-icons.mjs"
  },
  "dependencies": {
    "kleur": "^4.1.4"
  },
  "devDependencies": {
    "@vitebook/client": "workspace:*",
    "@vitebook/core": "workspace:*",
    "svelte": "^3.43.2",
    "vite": "2.7.0-beta.0"
  },
  "peerDependencies": {
    "@vitebook/client": "0.0.3",
    "@vitebook/core": "0.0.3",
    "svelte": "^3.43.2",
    "vite": "^2.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
