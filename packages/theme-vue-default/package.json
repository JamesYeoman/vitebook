{
  "name": "@vitebook/theme-vue-default",
  "version": "0.0.0",
  "keywords": [
    "esm",
    "plugin",
    "storybook",
    "theme-default",
    "vitebook",
    "vuebook"
  ],
  "bugs": {
    "url": "https://github.com/vitebook/vitebook/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vitebook/vitebook.git",
    "directory": "packages/theme-vue-default"
  },
  "license": "MIT",
  "contributors": [
    "Rahim Alwer <rahim.alwer@gmail.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*",
    "./client": "./src/client/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "client": [
        "./src/client/index.ts"
      ],
      "client/*": [
        "./src/client/*"
      ],
      "*": [
        "src/*"
      ]
    }
  },
  "files": [
    "src/"
  ],
  "dependencies": {
    "@vueuse/core": "^6.3.2"
  },
  "devDependencies": {
    "@vitebook/vue": "*",
    "vue": "^3.2.6"
  },
  "peerDependencies": {
    "@vitebook/vue": "*",
    "vue": "^3.2.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
